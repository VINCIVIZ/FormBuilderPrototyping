<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Prototype1Screens.AgeControl"
	d:DesignWidth="125" d:DesignHeight="26">

	<Grid x:Name="LayoutRoot">
		<Canvas>
			<TextBox Style="{StaticResource TextBox-Sketch}" TextWrapping="Wrap" Width="101" Height="21" Canvas.Left="24"/>
			<TextBlock Height="26" Style="{StaticResource TextBlock-Sketch}" TextWrapping="Wrap" Text="Age" Width="36" FontSize="16"/>
		</Canvas>
	</Grid>
</UserControl>